{
    "name": "crissanclick/shopify-app-skeleton",
    "license": "MIT",
    "type": "project",
    "description": "Shopify APP DDD skeleton",
    "require": {
        "php": ">8.1",
        "symfony/framework-bundle": "^4.3",
        "symfony/messenger": "^4.3",
        "symfony/dotenv": "^4.3",
        "symfony/yaml": "^4.3",
        "symfony/twig-bundle": "^4.3",
        "symfony/validator": "^4.3",
        "lambdish/phunctional": "^2.0",
        "ramsey/uuid": "*",
        "doctrine/dbal": "^2.9",
        "doctrine/orm": "^2.6",
        "ocramius/proxy-manager": "2.2.2",
        "elasticsearch/elasticsearch": "^7.3",
        "monolog/monolog": "^2.0",
        "shopify/shopify-api": "^5.1",
        "doctrine/cache": "^2.2"
    },
    "require-dev": {
        "ext-xdebug": "*",
        "roave/security-advisories": "dev-master",
        "behat/behat": "^3.5",
        "behat/mink-extension": "^2.3",
        "behat/mink-browserkit-driver": "^1.3",
        "friends-of-behat/symfony-extension": "^2.0",
        "phpunit/phpunit": "^8.2",
        "mockery/mockery": "^1.2",
        "wouterj/eloquent-bundle": "*"
    },
    "autoload": {
        "psr-4": {
            "Crissanclick\\Apps\\App\\Backend\\": "apps/app/backend/src",
            "Crissanclick\\Apps\\App\\Frontend\\": "apps/app/frontend",
            "Crissanclick\\": [
                "src"
            ]
        }
    }
}
